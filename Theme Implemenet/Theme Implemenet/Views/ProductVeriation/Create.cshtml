<body>
    <div class="container-scroller">
        <!-- partial:../../partials/_navbar.html -->
        <!-- partial -->
        <div class="container-fluid page-body-wrapper">
            <!-- partial:../../partials/_settings-panel.html -->
            <!-- partial -->
            <div class="main-panel" style="width:400%">
                <div class="content-wrapper container-fluid">
                    <div class="page-header">
                        <h3 class="page-title">
                            Product Form
                        </h3>
                        <nav aria-label="breadcrumb">
                            <ol class="breadcrumb">
                                @*                                <li class="breadcrumb-item"><a href="@Url.Action("Login","Account")">Signin</a></li>
                                *@
                            </ol>
                        </nav>
                    </div>
                    <div class="row">
                        <div class="card" style="width:400%">
                            <div class="card-body">
                                @* <p class="card-description">
                                Basic form elements
                                </p>*@
                                <form class="forms-sample" method="post" enctype="multipart/form-data">
                                    <div class="form-group">
                                        <label for="exampleInputName1">veriationName</label>
                                        <input asp-for="veriationName" class="form-control form-control-lg border-left-0"/>
                                        <span asp-validation-for="veriationName" class="text-danger"></span>
                                    </div>
                                    <div class="form-group">
                                        <label for="exampleInputName1">CostPrice</label>
                                        <input asp-for="costprice" class="form-control form-control-lg border-left-0" />
                                        <span asp-validation-for="costprice" class="text-danger"></span>
                                    </div>
                                    <div class="form-group">
                                        <label for="exampleInputName1">RetailerPrice</label>
                                        <input asp-for="RetailerPrice" class="form-control form-control-lg border-left-0" />
                                        <span asp-validation-for="RetailerPrice" class="text-danger"></span>
                                    </div>
                                    <div class="form-group">
                                        <label for="exampleInputName1">Quantity</label>
                                        <input asp-for="Quantity" class="form-control form-control-lg border-left-0" />
                                        <span asp-validation-for="Quantity" class="text-danger"></span>
                                    </div>
                                    <div class="form-group">
                                        <label>productId</label>
                                        <select asp-for="productId" class="form-control">
                                            @if (ViewBag.product != null)
                                            {
                                                foreach (var item in ViewBag.product)
                                                {
                                                    <option value="@item.productId">@item.productName</option>

                                                }
                                            }
                                        </select>
                                     <span asp-validation-for="productId" class="text-danger"></span>
                                    </div>
                                    <div class="form-group">

                                        <label asp-for="productsizeId"></label>
                                        <select asp-for="productsizeId" class="form-control">
                                            @if (ViewBag.size != null)
                                            {
                                                foreach (var item in ViewBag.size)
                                                {
                                                    <option value="@item.productsizeId">@item.productsizeName</option>

                                                }
                                            }
                                        </select>
                                        <span asp-validation-for="productcolorId" class="text-danger"></span>

                                    </div>
                                    <div class="form-group">
                                        <label asp-for="productcolorId"></label>
                                        <select asp-for="productcolorId" class="form-control">
                                            @if (ViewBag.color != null)
                                            {
                                                foreach (var item in ViewBag.color)
                                                {
                                                    <option value="@item.productcolorId">@item.productcolorName</option>

                                                }
                                            }
                                        </select>
                                        <span asp-validation-for="productcolorId" class="text-danger"></span>
                                    </div>
                                    <div class="form-group">
                                        <label asp-for="categoryId"></label>
                                        <select asp-for="categoryId" class="form-control">
                                            @if (ViewBag.cat != null)
                                            {
                                                foreach (var item in ViewBag.cat)
                                                {
                                                    <option value="@item.categoryId">@item.categoryName</option>

                                                }
                                            }
                                        </select>
                                        <span asp-validation-for="categoryId" class="text-danger"></span>

                                    </div>

                                    <div class="form-group">
                                        <label>SubcategoryId</label>

                                        <select asp-for="subcategoryId" class="form-control">
                                            <option>SelectSubCategory----</option>

                                            @if (ViewBag.sub != null)
                                            {
                                                foreach (var item in ViewBag.sub)
                                                {
                                                    <option value="@item.subcategoryId">@item.subcategoryName</option>

                                                }
                                            }
                                        </select>
                                        <span asp-validation-for="subcategoryId" class="text-danger"></span>
                                    </div>
                                    <div class="form-group">
                                        <label>categorystyleid</label>
                                        <select asp-for="categorystyleid" class="form-control">
                                            <option>Selectcategorystyleid----</option>
                                            @if (ViewBag.style != null)
                                            {
                                                foreach (var item in ViewBag.style)
                                                {
                                                 <option value="@item.categorystyleid">@item.categorystyleName</option>
                                                }
                                            }
                                        </select>
                                        <span asp-validation-for="categorystyleid" class="text-danger"></span>
                                    </div>
                                    <div class="form-group">
                                        <label>productmaterialId</label>
                                        <select asp-for="productmaterialId" class="form-control">
                                            <option>SelectproductmaterialId----</option>

                                            @if (ViewBag.material != null)
                                            {
                                                foreach (var item in ViewBag.material)
                                                {
                                                    <option value="@item.productmaterialId">@item.productmaterialname</option>

                                                }
                                            }
                                        </select>
                                        <span asp-validation-for="productmaterialId" class="text-danger"></span>
                                    </div>
                                    <div class="form-group">
                                        <label>productmaterialId</label>

                                        <select asp-for="productseasonId" class="form-control">
                                            <option>Selectproductseason----</option>

                                            @if (ViewBag.season != null)
                                            {
                                                foreach (var item in ViewBag.season)
                                                {
                                                    <option value="@item.productseasonId">@item.productseasonName</option>

                                                }
                                            }
                                        </select>
                                        <span asp-validation-for="productseasonId" class="text-danger"></span>
                                    </div>

                                    <div class="form-group">
                                        <label>productgenderId</label>
                                        <select asp-for="productgenderId" class="form-control">
                                            <option>SelectproductgenderId----</option>

                                            @if (ViewBag.gender != null)
                                            {
                                                foreach (var item in ViewBag.gender)
                                                {
                                                    <option value="@item.productgenderId">@item.productgenderName</option>
                                                }
                                            }
                                        </select>
                                        <span asp-validation-for="productgenderId" class="text-danger"></span>
                                    </div>
                                    <div class="form-group">
                                        <label>File upload</label>
                                        <input type="file" name="file" class="file-upload-default">
                                        <div class="input-group col-xs-12">
                                            <input type="text" class="form-control file-upload-info" disabled placeholder="Upload Image">
                                            <span class="input-group-append">
                                                <button class="file-upload-browse btn btn-primary" type="button">Upload</button>
                                            </span>
                                        </div>
                                    </div>
                                    <button type="submit" class="btn btn-primary mr-2">Submit</button>
                                    <button class="btn btn-light">Cancel</button>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- content-wrapper ends -->
        <!-- partial:../../partials/_footer.html -->
